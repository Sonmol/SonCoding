-- Pets_2 (Sell Pets)
local PetList = {}
for i,v in pairs(game:GetService("Players").LocalPlayer.petsFolder:GetChildren()) do
    table.insert(PetList, "All-"..v.Name)
end

Pets_2:Dropdown("Select Pets", PetList, function(val)
    _G.SellPet = val
end)

Pets_2:Toggle("Enable Sell Pets", function(val)
    _G.EnableSellPet = val
end)

