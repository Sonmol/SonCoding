-- Settings_2 (Players)
Settings_2:Slider("Walk Speed", 0, 1000, 15, function(t)
    game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = t
    while t do wait(1)
        game:GetService("Players").LocalPlayer.Character.Movement.Disabled = true
    end
end)

Settings_2:Slider("Jump Power", 0, 1000, 50, function(t)
    game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower = t
end)

Settings_2:Slider("Gravity", 0, 500, 196, function(t)
    workspace.Gravity = t
end)

