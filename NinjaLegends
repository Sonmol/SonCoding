-- Main_2 (Auto Karma)
local checkKill = Main_2:Label("")
local checkStreak = Main_2:Label("")

spawn(function()
    while game:GetService("RunService").Heartbeat:wait() do
        checkKill:Refresh("Killed : "..tostring(game.Players.LocalPlayer.leaderstats.Kills.Value))
        checkStreak:Refresh("Killed Streak : "..tostring(game.Players.LocalPlayer.leaderstats.Streak.Value))
    end
end)

local soulList = {
    "Bad";
    "Good";
}
    
Main_2:Dropdown("Select Karma", soulList, function(val)
    soulType = val
end)

Main_2:Toggle("Start Farm Karma", function(vu)
    checkKarma = vu
    if checkKarma == true and soulType == nil then
        Blacklib:Notification("Karma Notice.", "Please Select Karma Type First.")
    else
        _G.AutoKarma = vu
    end
end)


-----------------------------------------------------------------------------------

-- Main_3 />
local BossList = {'All'}
for i,v in pairs(game:GetService("Workspace").bossFolder:GetChildren()) do
    table.insert(BossList, v.Name)
end

local CheckBoss = Main_3:Dropdown("Select Boss", BossList, function(vu)
    _G.TypeBoss = vu
end)

Main_3:Button("Refresh Boss", function()
    CheckBoss:Clear()
    CheckBoss:Add('All')
    for i,v in pairs(game:GetService("Workspace").bossFolder:GetChildren()) do
        CheckBoss:Add(v.Name)
    end
end)

Main_3:Toggle("Start Auto Boss", function(vu)
    cb = vu
    if cb == true and _G.TypeBoss == "" then
        Blacklib:Notification("Auto Boss Notics..", "Please Select the Boss First.")
    else
        _G.AutoBoss = vu
    end
end)

-----------------------------------------------------------------------------------

-- Pets_1 (Auto Open Crystals)
local set = {}
local Crystals = {}
for i,v in next, game.workspace.mapCrystalsFolder:GetChildren() do 
    table.insert(Crystals, v.Name)
end

Pets_1:Dropdown("Select Crystals", Crystals, function(val)
    if val then
        set.SelectCrystal = val
    end
end)

Pets_1:Toggle("Start Open Crystals", function(op)
    if op == true then
        _G.OpenCrystales = true
    elseif op == false then
        _G.OpenCrystales = false
    end
    while _G.OpenCrystales do
        local oh1 = "openCrystal"
        local oh2 = set.SelectCrystal
        game:GetService("ReplicatedStorage").rEvents.openCrystalRemote:InvokeServer(oh1, oh2)
        
    end
end)

